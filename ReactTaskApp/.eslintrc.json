{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "plugin:react/recommended",
      "standard-with-typescript"
  ],
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "prettier"
  ],
  "rules": {
      "prettier/prettier" : "error", 
      "import/prefer-default-export" : "off",
      "no-console": "off",
      "no-param-reassign" :"error"
  }
}
